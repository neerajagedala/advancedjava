import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.SQLException;
// set classpath=C:\Users\neera\Downloads\mysql-connector-j-9.3.0.zip\mysql-connector-j-9.3.0\
class JdbcEx {

    public static void main (String[] args) throws SQLException, ClassNotFoundException{
     String driver=   "com.mysql.cj.jdbc.Driver";
     String url="jdbc:mysql://localhost:3306/school";
     String username="root";
     String pw="Neeraja@123.";
     try{
    Class.forName(driver);

     Connection conn=DriverManager.getConnection(url,username,pw);
     java.sql.Statement stmt=conn.createStatement();
     java.sql.ResultSet res= stmt.executeQuery("select * from class9");
     while(res.next()){
     System.out.println(res.getInt(1)+" "+res.getString(2));
     System.out.println();
     }
     conn.close();    
    }
    catch(Exception e){
        System.out.println(e);
    }
}

    
    
}
